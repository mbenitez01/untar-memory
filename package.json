{
  "name": "untar-memory",
  "version": "1.1.0",
  "description": "Untar a tarball to an in-memory filesystem",
  "main": "./lib/index.js",
  "scripts": {
    "test:node": "tape test/*.js | tap-spec",
    "test:lint": "snazzy",
    "test": "npm-run-all -p test:*",
    "prepublish": "mkdirp lib && buble -i src -o lib",
    "release": "xyz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/queckezz/untar-memory"
  },
  "author": "Fabian Eichenberger <fabian.eichenberger@gmail.com>",
  "dependencies": {
    "concat-stream": "^1.5.2",
    "file-type": "^3.8.0",
    "gunzip-maybe": "^1.3.1",
    "memory-fs": "^0.4.1",
    "tar": "^2.2.1"
  },
  "devDependencies": {
    "npm-run-all": "^2.3.0",
    "tap-spec": "^4.1.1",
    "mkdirp": "^0.5.1",
    "snazzy": "^4.0.0",
    "buble": "^0.13.1",
    "tape": "^4.6.0",
    "xyz": "^1.0.0"
  },
  "keywords": [
    "tar",
    "untar",
    "tarball",
    "tgz",
    "archive",
    "extract"
  ],
  "license": "MIT"
}
